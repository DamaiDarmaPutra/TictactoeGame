package TicTacToe;

public class Papan {

	public static char[][] papan = new char[3][3];


	public void printBoard() {

	    System.out.println("-------------");
	    for (int i = 0; i < 3; i++) {
	        System.out.print("| ");
	        for (int j = 0; j < 3; j++) {

	            System.out.print(papan[i][j] + " | ");

	        }
	        System.out.println();

	    }
	    System.out.println("-------------");

	}
	
}
