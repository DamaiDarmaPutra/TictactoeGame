package TicTacToe;

public class Terisi {

	char[][] papan = Papan.papan;

	public boolean OutOfBoard(int row, int col) {
	    if (row > 3 || col > 3) {
	        System.err.println("There is no row or column");
	        return true;

	    }
	    return false;

	}
	public boolean isFull(int row, int col) {
	    if (row > 3 || col > 3) {
	        System.err.println("There is no row or column");
	        return true;

	    }
	    return false;
	}
}
